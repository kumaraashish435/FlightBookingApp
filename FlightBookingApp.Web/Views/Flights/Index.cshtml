     @model IEnumerable<Flight>

<h1>Flights</h1>
<table class="table">
    <thead>
        <tr>
            <th>Flight Number</th>
            <th>Destination</th>
            <th>Departure Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var flight in Model)
        {
            <tr>
                <td>@flight.FlightNumber</td>
                <td>@flight.Destination</td>
                <td>@flight.DepartureTime</td>
            </tr>
        }
    </tbody>
</table>
